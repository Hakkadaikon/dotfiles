colorscheme habamax
syntax on

set relativenumber
set laststatus=2
set softtabstop=4
set shiftwidth=4
set cmdheight=2
set smarttab
set expandtab
set matchpairs+=<:>
set list
set listchars=tab:»-,trail:-,eol:↲,extends:»,precedes:«,nbsp:%

inoremap { {}<Left>
inoremap ( ()<Left>
inoremap [ []<Left>
inoremap " ""<Left>
inoremap ' ''<Left>
